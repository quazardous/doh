# Project Configuration
PROJECT_NAME={{PROJECT_NAME}}

# Container Naming (for multi-branch development)
# Example: my-app-branch1-app-1, my-app-main-app-1
APP_CONTAINER={{PROJECT_NAME}}-app-1

# Traefik
EXTERNAL_HTTP_PORT=80
EXTERNAL_HTTPS_PORT=443

# Application Environment
APP_ENV=development

# PostgreSQL Configuration (if using PostgreSQL)
# POSTGRES_DB={{PROJECT_NAME}}_dev
# POSTGRES_USER=dev  
# POSTGRES_PASSWORD=secret

# MySQL Configuration (if using MySQL)  
# MYSQL_DATABASE={{PROJECT_NAME}}_dev
# MYSQL_USER=dev
# MYSQL_PASSWORD=secret
# MYSQL_ROOT_PASSWORD=secret

# Redis Configuration (if using Redis)
# (Redis typically needs no additional config)